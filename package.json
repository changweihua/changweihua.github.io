{
  "name": "changweihua.github.io",
  "author": "常伟华 (https://github.com/changweihua)",
  "description": "This is for study gulp project !",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/changweihua.github.io"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "license": "ISC",
  "scripts": {
    "docs:dev": "vitepress --mode development --host",
    "docs:prepare": "esno scripts/prepare.ts",
    "docs:build": "set NODE_OPTIONS=--max-old-space-size=4096 && vitepress build",
    "docs:test": "vitest --ui",
    "docker:build": "docker build -t changweihua-github:latest . -f Dockerfile",
    "docs:preview": "vitepress preview --host",
    "docs:pdf": "npm run docs:build && node generate-pdf.js",
    "commit": "git add . && cz-customizable",
    "docs:vmark": "vmark",
    "docs:version": "esno ./utils/build-version.ts",
    "docs:vbuild": "vmark && vitepress build",
    "release": "npx release-it",
    "inspect": "npx node-modules-inspector",
    "changelog": "conventional-changelog -p gitmoji-config -i CHANGELOG.md -s && git add CHANGELOG.md",
    "register:components": "vitepress-rc",
    "pkg:patch": "patch-package",
    "deploy:win": "powershell scripts/autoDeploy.bat",
    "font:prepare": "node scripts/font_extractor.cjs",
    "font:build": "node scripts/font_reset.cjs && npx font-spider fonts-spider/index.html --no-backup && node scripts/font_move.cjs",
    "font:test": "node --disable-warning=ExperimentalWarning --env-file .env scripts/env-test.ts",
    "lint": "eslint --fix --ext .ts,.js,.vue src test/unit",
    "fix-memory-limit": "cross-env LIMIT=8048 increase-memory-limit",
    "prepare": "husky",
    "license:checker": "license-checker --trace-deprecation --json > ./licenses.json",
    "license:checker:csv": "license-checker --csv > ./licenses.csv",
    "test:unit": "vitest --ui",
    "mkcert:install": "mkcert -install",
    "mkcert:uninstall": "mkcert -uninstall",
    "export-pdf": "press-export-pdf export ./zh-CN/gallery",
    "ncu": "npx npm-check-updates",
    "taze": "npx taze",
    "deploy": "node ./deploy/index.ts"
  },
  "imports": {
    "#*": "./*"
  },
  "lint-staged": {
    "*": "npm run lint"
  },
  "type": "module",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    }
  },
