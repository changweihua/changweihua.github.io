  "license": "ISC",
  "scripts": {
    "docs:dev": "vitepress --mode development --host",
    "docs:prepare": "esno scripts/prepare.ts",
    "docs:build": "set NODE_OPTIONS=--max-old-space-size=4096 && vitepress build",
    "docs:test": "esno ./utils/build-version.ts",
    "docs:preview": "vitepress preview",
    "docs:pdf": "npm run docs:build && node generate-pdf.js",
    "commit": "git add . && cz-customizable",
    "docs:version": "vmark",
    "docs:vbuild": "vmark && vitepress build",
    "release": "release-it",
    "changelog": "conventional-changelog -p gitmoji-config -i CHANGELOG.md -s && git add CHANGELOG.md",
    "register:components": "vitepress-rc",
    "pkg:patch": "patch-package",
    "deploy:win": "powershell scripts/autoDeploy.bat",
    "font:prepare": "node scripts/font_extractor.cjs",
    "font:build": "node scripts/font_reset.cjs && font-spider font/index.html --no-backup && node scripts/font_move.cjs",
    "lint": "eslint --fix --ext .ts,.js,.vue src test/unit",
    "fix-memory-limit": "cross-env LIMIT=8048 increase-memory-limit",
    "prepare": "husky",
    "license:checker": "license-checker --trace-deprecation --json > ./licenses.json",
    "license:checker:csv": "license-checker --csv > ./licenses.csv",
    "test:unit": "vitest --ui",
    "mkcert:install": "mkcert -install",
    "mkcert:uninstall": "mkcert -uninstall",
    "export-pdf": "press-export-pdf export ./zh-CN/gallery"
  },
  "imports": {
    "#*": "./*"
  },
  "lint-staged": {
    "*": "npm run lint"
  },
  "type": "module",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    }
  },
