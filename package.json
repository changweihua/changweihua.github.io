{
  "scripts": {
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview",
    "register:components": "vitepress-rc",
    "pkg:patch": "patch-package",
    "deploy:win": "powershell bin/autoDeploy.bat",
    "commit": "git add . && cz",
    "release": "release-it",
    "version": "conventional-changelog -p custom-config -i CHANGELOG.md -s -n ./changelog-option.js && git add CHANGELOG.md",
    "postversion": "git push --tags",
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -n ./changelog-option.js && git add CHANGELOG.md"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "@release-it/conventional-changelog": {
      "infile": "CHANGELOG.md",
      "ignoreRecommendedBump": true,
      "strictSemVer": true,
      "preset": {
        "name": "conventionalcommits",
        "types": [
          {
            "type": "feat",
            "section": "✨新功能"
          },
          {
            "type": "fix",
            "section": "🐛问题修复"
          },
          {
            "type": "docs",
            "section": "📚文档"
          },
          {
            "type": "chore",
            "section": "🔧配置文件"
          },
          {
            "type": "style",
            "section": "💄修改样式"
          },
          {
            "type": "release",
            "hidden": "true"
          }
        ]
      }
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@iconify-json/carbon": "^1.1.18",
    "@iconify-json/logos": "^1.1.33",
    "@iconify/vue": "^4.1.1",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/node": "^20.3.3",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^3.0.0",
    "cssnano": "^6.0.1",
    "cz-customizable": "^7.0.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "patch-package": "^7.0.0",
    "postcss": "^8.4.24",
    "release-it": "^15.11.0",
    "tailwindcss": "^3.3.2",
    "unocss": "^0.53.4",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-icons": "^0.16.3",
    "unplugin-vue-components": "^0.25.1",
    "vitepress": "^1.0.0-beta.4",
    "vitepress-plugin-autobar": "^1.0.8",
    "vue": "^3.3.4"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@iconify/iconify": "^3.1.1",
    "@vitepress-demo-preview/component": "^2.3.0",
    "@vitepress-demo-preview/plugin": "^1.2.0",
    "animate.css": "^4.1.1",
    "ant-design-vue": "^3.2.20",
    "markdown-it-custom-attrs": "^1.0.2",
    "vitepress-markdown-timeline": "^1.2.1",
    "vitepress-plugin-nprogress": "^0.0.4"
  },
  "version": "0.0.4"
}
