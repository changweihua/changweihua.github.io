{
  "markdown.copyFiles.overwriteBehavior": "overwrite",
  "markdown.copyFiles.destination": {
    "/**/*": "/public/images/cmono-${fileName}"
  },
  "unocss.root": "packages/client",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "package.json": "tsconfig.*.json,*.cjs,*.js,*.json,*.config.ts,*.config.cjs,.preettierrc,.eslintrc.cjs,.eslintrc,.gitignore,.editorconfig",
    "index.html": "index.html,*.css",
    "auto-imports.d.ts":"auto-imports.d.ts,components.d.ts",
    "index.md":"index.md,CONTRIBUTING.md,CODE_OF_CONDUCT.md,CONTRIBUTING.zh-CN.md,README.md,CHANGELOG.md,PRIVACY_POLICY.md,TERMS_OF_SERVICE.md"
  }
  //   // 拖拽行为
  //   "markdown.editor.drop.copyIntoWorkspace": "never",
  //   // 粘贴行为
  //   "markdown.editor.filePaste.copyIntoWorkspace": "never"
}
